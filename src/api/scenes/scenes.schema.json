{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Sampler",
    "description": "Sampler Object",

    "required": [
       "timestamp"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database id of scene"
        },
        "tricks": {
            "$id": "#/properties/tricks",
            "type": "string",
            "title": "Tricks of a scene"
        },
        "timestamp": {
            "$id": "#/properties/timestamp",
            "type": "integer",
            "title": "Timestamp of tricks"
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date of user"

        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date user updated last"
        }
    },
    "additionalProperties": false
}