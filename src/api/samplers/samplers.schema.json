{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Sampler",
    "description": "Sampler Object",

    "required": [
        "name",
        "created_by",
        "url"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database id of user"
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "Sampler url"
        },
        "created_by": {
            "$id": "#/properties/created_by",
            "type": "string",
            "title": "Author(s) of a sampler."
        },
        "url": {
            "$id": "#/properties/url",
            "type": "string",
            "title": "Sampler url"
            
        },
        "runtime": {
            "$id": "#/properties/runtime",
            "type": "string",
            "title": "Sampler runtime"
        },
        "upload_date": {
            "$id": "#/properties/upload_date",
            "type": "string",
            "title": "Upload date of sampler"
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date of user"

        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date user updated last"
        }
    },
    "additionalProperties": false
}